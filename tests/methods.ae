struct Rectangle {
    width: i32
    height: i32
}

def Rectangle::area(this): i32 {
    return this.width * this.height
}

def Rectangle::scale(this, factor: i32) {
    this.width = this.width * factor
    this.height = this.height * factor
}

def main() {
    let x: Rectangle
    x.width = 23
    x.height = 3
    println("%d", x.area())

    x.scale(2)
    let z = &x
    z.scale(2)

    println("%d", z.area())
}
